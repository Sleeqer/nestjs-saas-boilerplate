{
  "name": "nest-rest-typeorm-boilerplate",
  "version": "0.0.1",
  "description": "nest typeorm boilerplate",
  "author": "dorin.musteata",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "start:prod": "node dist/main.js",
    "typedocs": "npx compodoc -p tsconfig.json -s -d docs",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --detectOpenHandles --forceExit"
  },
  "dependencies": {
    "@hapi/joi": "17.1.1",
    "@nestjs/common": "7.6.13",
    "@nestjs/core": "7.6.13",
    "@nestjs/event-emitter": "^0.0.3",
    "@nestjs/graphql": "^7.9.11",
    "@nestjs/jwt": "7.2.0",
    "@nestjs/passport": "7.1.5",
    "@nestjs/platform-fastify": "7.6.13",
    "@nestjs/platform-socket.io": "^7.6.13",
    "@nestjs/swagger": "4.7.15",
    "@nestjs/throttler": "^1.1.1",
    "@nestjs/typeorm": "7.1.5",
    "@nestjs/websockets": "^7.6.13",
    "amqplib": "^0.7.0",
    "apollo-server-fastify": "next-v3",
    "change-case": "^4.1.2",
    "class-transformer": "0.4.0",
    "class-validator": "0.13.1",
    "crypto": "1.0.1",
    "dotenv": "8.2.0",
    "fastify-cors": "^5.2.0",
    "fastify-helmet": "5.2.0",
    "fastify-rate-limit": "5.4.1",
    "fastify-swagger": "4.3.3",
    "graphql": "^15.5.0",
    "graphql-tools": "^7.0.4",
    "gravatar": "1.8.1",
    "ioredis": "^4.23.0",
    "mongodb": "^3.6.4",
    "mysql": "2.18.1",
    "nest-access-control": "2.0.2",
    "passport": "0.4.1",
    "passport-jwt": "4.0.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "6.6.6",
    "socket.io": "^3.1.2",
    "socket.io-redis": "^6.0.1",
    "typeorm": "0.2.31",
    "uuid": "^8.3.2",
    "winston": "3.3.3",
    "winston-daily-rotate-file": "4.5.0",
    "winston-mongodb": "^5.0.7"
  },
  "devDependencies": {
    "@compodoc/compodoc": "1.1.11",
    "@nestjs/testing": "7.6.13",
    "@types/amqplib": "^0.5.17",
    "@types/dotenv": "8.2.0",
    "@types/gravatar": "1.8.1",
    "@types/hapi__joi": "17.1.6",
    "@types/ioredis": "^4.22.0",
    "@types/jest": "26.0.20",
    "@types/mongodb": "^3.6.8",
    "@types/node": "13.13.45",
    "@types/passport-jwt": "3.0.5",
    "@types/socket.io": "^2.1.13",
    "@types/socket.io-redis": "^1.0.27",
    "@types/supertest": "2.0.10",
    "chai": "4.3.3",
    "jest": "26.6.3",
    "nodemon": "2.0.7",
    "prettier": "2.2.1",
    "supertest": "6.1.3",
    "ts-jest": "26.5.2",
    "ts-loader": "8.0.17",
    "ts-node": "9.1.1",
    "tsc-watch": "4.2.9",
    "tsconfig-paths": "3.9.0",
    "tslint": "6.1.3",
    "typescript": "4.2.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
